<?xml version="1.0"?>
<MoTecM1BuildModuleSet Locale="English_Australia.1252" DefaultLocale="C" FileFormat="10103" Name="MoTeC LTC" VersionMajor="1" VersionMinor="6" BuildNumber="6">
 <System VersionMajor="1" VersionMinor="3" VersionRelease="16" VersionBuild="0034"/>
 <BuildHelp>
<![CDATA[<h4 class="device_help">Current Version</h4>
            <ul class="device_help">
              <li class="device_help">Fix 'Diagnostic' reporting <em class="device_enum">Disabled</em> when 'Sensor State' is <em class="device_enum">Pump Start</em> with LTCN
                firmwares newer than 1.10</li>
            </ul>
            <h4 class="device_help">1.6.0</h4>
            <ul class="device_help">
              <li class="device_help">Use MoTeC Types 1.4</li>
            </ul>
            <h4 class="device_help">1.5.0</h4>
            <ul class="device_help">
              <li class="device_help">Initial release.</li>
            </ul>]]>
 </BuildHelp>
