  <Type Name="LTC Diagnostic Enumeration" Storage="enum" Default="Not in Use">
   <Enum Name="OK" ContainerOrder="12">
    <Help>
<![CDATA[<p class="device_help">The LTC or PLM is operating correctly.</p>]]>
    </Help>
   </Enum>
   <Enum Name="CAN Bus Timeout" ContainerOrder="10" Tags="Fault">
    <Help>
<![CDATA[<p class="device_help">No LTC or PLM CAN messages have been received for longer than 1 second.</p>]]>
    </Help>
   </Enum>
   <Enum Name="Heater Open Fault" ContainerOrder="5" Tags="Fault">
    <Help>
<![CDATA[<p class="device_help">The LTC has detected that the sensor heater is open circuit.</p>
            <p class="device_help">Refer to the LTC documentation for more information.</p>]]>
    </Help>
   </Enum>
   <Enum Name="Heater VBAT Short Fault" ContainerOrder="6" Tags="Fault">
    <Help>
<![CDATA[<p class="device_help">The LTC has detected that the sensor heater is shorted to VBAT.</p>
            <p class="device_help">Refer to the LTC documentation for more information.</p>]]>
    </Help>
   </Enum>
   <Enum Name="Heater GND Short Fault" ContainerOrder="7" Tags="Fault">
