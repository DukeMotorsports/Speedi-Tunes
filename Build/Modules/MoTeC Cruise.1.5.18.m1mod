        <Props Qty="ratio/(m/s/s)" Validation="MinMax" ValMin="1.00000000000000000e-003" ValMax="1.019720000000000049e+01">
         <Locale>
          <Default Unit="%/G" Min="0.00000000000000000e+000" Max="1.01970000000000010e+000"/>
         </Locale>
        </Props>
